#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long long int t;
	cin>>t;
	long long int x,y;
	while(t--){
	    cin>>x>>y;
	    int z=0;
	    int c=0;
	    if(x==y){
	        cout<<"0"<<endl;
	        //break;
	    }
	    else{
	        while(x<y){
	            x=x+8;
	            c++;
	        }
	    cout<<c<<endl;}
	  
	}
	return 0;
}
