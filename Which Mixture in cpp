#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a,b;
	while(t--){
	    cin>>a>>b;
	    if(a>0 && b>0){
	        cout<<"Solution"<<endl;
	    }
	    else if(b>0){
	        cout<<"Liquid"<<endl;
	    }
	    else{
	        cout<<"Solid"<<endl;
	    }
	}
	return 0;
}
