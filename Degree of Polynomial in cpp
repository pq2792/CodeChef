#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int n;
	while(t--){
	    cin>>n;
	    double a[n];
	    int l=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        if(a[i]!=0){
	            l=i;
	        }
	        
	    }
	    cout<<l<<endl;
	}
	return 0;
}
