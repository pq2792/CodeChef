#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--)
	{
	    int a[3];
	    int n=0,p=0;
	    for(int i=0;i<3;i++)
	    cin >> a[i];
	    for(int i=0 ;i<3;i++)
	    {
	        if(a[i]<0)
	        n++;
	        else if(a[i]>0)
	        p++;
	    }
	    if(p>=1 and n>=1)
	    cout<<"YES"<<endl;
	    else
	    cout<<"NO"<<endl;
	}

	return 0;
}
