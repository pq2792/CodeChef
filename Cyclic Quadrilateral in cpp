#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long int t;
	cin>>t;
	int a,b,c,d;
	while(t--){
	    cin>>a>>b>>c>>d;
	    int e=0;
	    int f=0;
	    e=a+c;
	    f=b+d;
	    if(e==180 && f==180){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
