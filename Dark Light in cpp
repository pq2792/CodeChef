#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	long long int n;
	int k;
	while(t--){
	    cin>>n>>k;
	    if(k==0){
	        if(n%4==0){
	            cout<<"off"<<endl;
	        }
	        else{
	            cout<<"on"<<endl;
	        }
	    }
	    else if(k==1){
	        if(n%4==0){
	            cout<<"on"<<endl;
	        }
	        else{
	            cout<<"Ambiguous"<<endl;
	        }
	    }
	    
	}
	return 0;
}
