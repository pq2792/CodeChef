#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int x,y,z;
	while(t--){
	    cin>>x>>y>>z;
	    int d=0;
	    d=z*2;
	    int e=0;
	    e=x+d;
	    if(e>=y){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
