#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a,c;
	//int b;
	while(t--){
	    int b=0;
	    cin>>a>>c;
	    b=(a+c)/2;
	    //cout<<b;
	    int d=0;
	    int e=0;
	    d=b-a;
	    e=c-b;
	    
	    if(d==e){
	        cout<<b<<endl;
	    }
	    else{
	        cout<<"-1"<<endl;
	    }
	}
	return 0;
}
