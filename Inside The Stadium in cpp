#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    float c=0,sum=0;
	    for(int i=1;i<=n;i++){
	        cin>>a[i];
	    }
	    for(int i=1;i<=n;i++){
	        sum+=a[i];
	        //cout<<sum<<" ";
	        float d = (sum/i)*100;
	        //cout<<d<<" ";
	        if(d==100){
	            c++;
	        }
	    }
	    cout<<c<<endl;
	}
	return 0;
}
