#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,x,d=1;
	    cin>>n>>x;
	    int s[n],r[n];
	    for(int i=0;i<n;i++){
	        cin>>s[i]>>r[i];
	    }
	    for(int i=0;i<n;i++){
	        if(s[i]<=x){
	            d=max(d,r[i]);
	        }
	    }
	    cout<<d<<endl;
	}
	return 0;
}
