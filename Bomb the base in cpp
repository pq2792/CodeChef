#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,x;
	    cin>>n>>x;
	    int a[n],max=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	        if(a[i]<x){
	            max=i+1;
	        }
	    }
	    cout<<max<<endl;
	}
	return 0;
}
