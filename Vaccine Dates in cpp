#include <iostream>
using namespace std;

int main() {
	// your code goes here
	long int t;
	cin>>t;
	long long int d,l,r;
	while(t--){
	    cin>>d>>l>>r;
	    if(d>=l && d<=r){
	        cout<<"Take second dose now"<<endl;
	    }
	    if(d>l && d>r){
	        cout<<"Too Late"<<endl;
	    }
	    if(d<l && d<r){
	        cout<<"Too Early"<<endl;
	    }
	}
	return 0;
}
