#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int e;
	cin>>e;
	
	while(e--){
	    string s,t;
	    cin>>s;
	    cin>>t;
	    for(int i=0;i<5;i++){
	        if(s[i]==t[i]){
	            cout<<"G";
	        }
	        else{
	            cout<<"B";
	        }
	       
	    }
	    cout<<endl;
	}
	return 0;
}
