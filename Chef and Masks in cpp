#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int x,y;
	while(t--){
	    cin>>x>>y;
	    int c=0;
	    int d=0;
	    c=x*100;
	    d=y*10;
	    if(c>=d){
	        cout<<"cloth"<<endl;
	    }
	    else{
	        cout<<"disposable"<<endl;
	    }
	}
	return 0;
}
