#include<bits/stdc++.h>
using namespace std;
int main(){
    int t;
    cin>>t;
    int n;
    while(t--){
        cin>>n;
        int te;
        te=n;
        int rev=0;
        while(n>0){
            int s=n%10;
            rev=(rev*10)+s;
            n=n/10;
            
        }
        
        if(rev==te){
            cout<<"wins"<<endl;
        }
        else{
            cout<<"loses"<<endl;
        }
    }
}
