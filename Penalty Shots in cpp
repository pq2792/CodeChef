#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	
	while(t--){
	    int a[10];
	    for(int i=0;i<10;i++){
	        cin>>a[i];
	    }
	    int c=0;
	    for(int i=0;i<10;i=i+2){
	        c+=a[i];
	    }
	    int d=0;
        for(int i=1;i<=10;i=i+2){
	        d+=a[i];
	    }
	    if(c>d){
	        cout<<"1"<<endl;
	    }
	    else if(c<d){
	        cout<<"2"<<endl;
	    }
	    else{
	        cout<<"0"<<endl;
	    }
	}
		
	
	return 0;
}
