#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    double a,b;
	cin>>a>>b;
	char c;
	cin>>c;
	if(c=='+'){
	    cout<<a+b;
	}
	else if(c=='-'){
	    cout<<a-b;
	}
	else if(c=='*'){
	    cout<<fixed<<setprecision(1)<<a*b;
	}
	else if(c=='/'){
	    cout<<fixed<<setprecision(8)<<a/b;
	}
	return 0;
}
