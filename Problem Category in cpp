#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a;
	while(t--){
	    cin>>a;
	    if(a<100){
	        cout<<"Easy"<<endl;
	    }
	    else if(a>=100 && a<200){
	        cout<<"Medium"<<endl;
	    }
	    else{
	        cout<<"Hard"<<endl;
	    }
	}
	return 0;
}
