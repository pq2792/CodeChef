#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    int a=0,b=0;
	    for(int i=0;i<s.length();i++){
	        if(s[i]=='0'){
	            a++;
	        }
	        else{
	            b++;
	        }
	        
	    }
	    if(a==1 || b==1){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
