#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int a,b;
	    cin>>a>>b;
	    for(int i=1;i<=1000;i++){
	        if(i%2!=0){
	            a-=i;
	            if(a<0){
	                cout<<"Bob"<<endl;
	                break;
	            }
	        }
	        else{
	            b-=i;
	            if(b<0){
	                cout<<"Limak"<<endl;
	                break;
	            }
	        }
	    }
	    
	   
	}
	return 0;
}
