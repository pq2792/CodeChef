#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a[7];
	while(t--){
	    int c=0,d=0;
	    for(int i=0;i<7;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<7;i++){
	        if(a[i]==1){
	            c++;
	        }
	        else{
	            d++;
	        }
	    }
	    if(c>=d){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
