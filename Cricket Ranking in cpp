#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	
	while(t--){
	    int a[3],b[3],c=0,d=0;
	    for(int i=0;i<3;i++)
	    {
	        cin>>a[i];
	    }
	    for(int i=0;i<3;i++)
	    {
	        cin>>b[i];
	    }
	    for(int i=0;i<3;i++)
	    {
	        if(a[i]>b[i])
	        {
	            c++;
	        }
	        else if(a[i]<b[i])
	        {
	            d++;
	        }
	    }
	
	    if(c>d)
	    {
	        cout<<"A"<<endl;
	    }
	    else if(c<d)
	    {
	        cout<<"B"<<endl;
	    }
    }
	return 0;
}
