#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int h,x,y,c;
	    cin>>h>>x>>y>>c;
	    int d=y/2;
	    int e=x+d;
	    int f=h*e;
	    if(f<=c){
	        cout<<"yes"<<endl;
	    }
	    else{
	        cout<<"no"<<endl;
	    }
	}
	return 0;
}
