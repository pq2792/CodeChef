#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	   int a,b,c,d;
	   cin>>a>>b>>c>>d;
	   int e=0;
	   for(int i=1;i<=8;i++){
	       if((i>=a && i<=b) || (i>=c && i<=d) ){
	           e++;
	       }
	   }
	   cout<<e<<endl;
	}
	return 0;
}
