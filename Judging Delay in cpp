#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int s[n],j[n],c=0;
	    for(int i=0;i<n;i++){
	        cin>>s[i]>>j[i];
	        int d=j[i]-s[i];
	        if(d>5){
	            c++;
	        }
	    }
	    cout<<c<<endl;
	    
	}
	return 0;
}
