#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int a;
	cin>>a;
	while(a--){
	    int b,c;
	    cin>>b>>c;
	    if(b<c){
	        while(b<c){
	            b=b*2;
	        }
	        if(b==c){
	            cout<<"Yes"<<endl;
	        }else{
	            cout<<"No"<<endl;
	        }
	    }else{
	            while(c<b){
	                c=c*2;
	            }
	            if(b==c){
	                cout<<"Yes"<<endl;
	            }else{
	                cout<<"No"<<endl;
	            }
	    }
	}
	return 0;
}
