#include <iostream>
using namespace std;

int main() {
	// your code goes here
	string s;
	cin>>s;
	int t;
	cin>>t;
	while(t--){
	    string r;
	    cin>>r;
	    int c=0;
	    int e=r.length();
	    for(int i=0;i<r.length();i++){
	        for(int j=0;j<s.length();j++){
	            if(r[i]==s[j]){
	                c++;
	                break;
	            }
	        }
	    }
	    if(c==e){
	        cout<<"Yes"<<endl;
	    }
	    else{
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
