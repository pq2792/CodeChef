#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s;
	    cin>>s;
	    sort(s.begin(),s.end());
	    int c=0,flag;
	    for(int i=0;i<s.length();i+=2){
	        if(s[i]==s[i+1]){
	            flag=1;
	        }
	        else{
	            flag=0;
	            break;
	        }
	    }
	    if(flag){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
