#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int x;
    while(t--){
        cin>>x;
        if(x<4){
            cout<<"mild"<<endl;
        }
        else if(x>=4 && x<7){
            cout<<"medium"<<endl;
        }
        else{
            cout<<"hot"<<endl;
        }
    }
	return 0;
}
