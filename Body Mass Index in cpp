#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	long long int t;
	long int m;
	int h;
	cin>>t;
	while(t--){
	    cin>>m>>h;
	    int d=m/(pow(h,2));
	    if(d<=18){
	        cout<<"1"<<endl;
	    }
	    else if(d>18 && d<=24){
	        cout<<"2"<<endl;
	    }
	    else if(d>24 && d<=29){
	        cout<<"3"<<endl;
	    }
	    else{
	        cout<<"4"<<endl;
	    }
	    
	}
	return 0;
}
