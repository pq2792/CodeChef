#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int min=a[0],c=0;
	    for(int i=0;i<n;i++){
	        if(a[i]<=min){
	            min=a[i];
	            c++;
	        }
	    }
	    cout<<c<<endl;
	}
	return 0;
}
