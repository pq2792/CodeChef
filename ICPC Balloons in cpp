#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    int c=0;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        if(a[i]==1 || a[i]==2 || a[i]==3 || a[i]==4 || a[i]==5 ||a[i]==6 || a[i]==7)
	        {
	            c=i+1;;
	        }
	    }
	    cout<<c<<endl;
	}
	return 0;
}
