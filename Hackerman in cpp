#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int x,y;
	while(t--){
	    cin>>x>>y;
	    int z=0;
	    z=x+y;
	    int c=0;
	    for(int i=1;i<=z;i++){
	        if(z%i==0){
	            c+=1;
	        }
	    }
	    if(c>2){
	        cout<<"bob"<<endl;
	    }
	    else{
	        cout<<"alice"<<endl;
	    }
	}
	return 0;
}
