#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	float a,b,x,y;
	while(t--){
	    cin>>a>>b>>x>>y;
	    if((a/x)<(b/y)){
	        cout<<"chef"<<endl;
	    }
	    else if((a/x)>(b/y)){
	        cout<<"chefina"<<endl;
	    }
	    else{
	        cout<<"both"<<endl;
	    }
	}
	
	return 0;
}
