#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a,b,c;
	while(t--){
	    cin>>a>>b>>c;
	    if(a<b && a<c){
	        cout<<"draw"<<endl;
	    }
	    else if(b<a && b<c){
	        cout<<"bob"<<endl;
	    }
	    else{
	        cout<<"alice"<<endl;
	    }
	    
	}
	return 0;
}
