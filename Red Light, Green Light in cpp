#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	std::cin >> t;
	while(t--){
	    int n,k;
	    cin>>n>>k;
	    int h[n];
	    int c=0;
	    for(int i=0;i<n;i++){
	        cin>>h[i];
	        if(h[i]>k){
	            c++;
	        }
	    }
	    cout<<c<<endl;
	}
	
	
	return 0;
}
