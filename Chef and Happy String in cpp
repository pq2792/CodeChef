#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    string s;
	    cin>>s;
	    int c=0;
	    int l=s.length();
	    for(int i=0;i<l;i++){
	        if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'){
	            c++;
	            if(c==3){
	                cout<<"Happy"<<endl;
	                break;
	            }
	        }
	        else{
	            c=0;
	        }
	    }
	    if(c!=3){
	        cout<<"Sad"<<endl;
	    }
	}
	return 0;
}
