#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	int x,y;
	cin>>t;
	while(t--){
	    cin>>x>>y;
	    int c=2*x;
	    int d=5*y;
	    if(c>d){
	        cout<<"chocolate"<<endl;
	    }
	    else if(c<d){
	        cout<<"candy"<<endl;
	    }
	    else{
	        cout<<"either"<<endl;
	    }
	}
	return 0;
}
