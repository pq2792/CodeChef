#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    long long int x;
	    cin>>x;
	    string s;
	    cin>>s;
	    //int n=x.size();
	    int c=0,d=0,e=0;
	    for(int i=0;i<14;i++){
	        if(s[i]=='D'){
	            c++;
	        }
	        else if(s[i]=='C'){
	            d++;
	        }
	        else if(s[i]=='N'){
	            e++;
	        }
	    }
	    if(d>e){
	        cout<<60*x<<endl;
	    }
	    else if(d==e){
	        cout<<55*x<<endl;
	    }
	    else{
	        cout<<40*x<<endl;
	    }
	}
	return 0;
}
