#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	long long int a[n];
	int sum=0,count=0;
	for(int i=1;i<=n;i++){
	    cin>>a[i];
	    sum=sum+a[i];
	    count=count+i;
	}
	//cout<<sum<<" "<<count;
	if(sum==count){
	    cout<<"YES"<<endl;
	}
	else{
	    cout<<"NO"<<endl;
	}
	return 0;
}
