#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int n;
	while(t--){
	    string s;
	    cin>>n;
	    cin>>s;
	    for(int i=0;i<n/2;i++){
	        if(s[i*2]=='0'){
	            if(s[i*2+1]=='0'){
	                cout<<'A';
	            }
	            else if(s[i*2+1]=='1'){
	                cout<<'T';
	            }
	        }
	        else if(s[i*2]=='1'){
	            if(s[i*2+1]=='0'){
	                cout<<'C';
	            }
	            else if(s[i*2+1]=='1'){
	                cout<<'G';
	            }
	        }
	    }
	    cout<<endl;
	}
	return 0;
}
