#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,c=0;
	    cin>>n;
	    string a;
	    cin>>a;
	    for(int i=0;i<n;i++){
	        if(a[i]=='1'){
	            c++;
	        }
	    }
	    int e=120-n+c;
	    float f;
	    f=(e*100)/120;
	    //cout<<f<<endl;
	    if(f>=75){
	        cout<<"YES"<<endl;
	    }
	    else if(f<75){
	        cout<<"NO"<<endl;
	    }
	    
	    
	}
	return 0;
}
