#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	   int n,x,y;
	   cin>>n>>x>>y;
	   int flag=0;
	   for(int i=0;i<=n;i++){
	       if(i*x==y){
	           flag=1;
	           break;
	       }
	       else{
	           flag=0;
	       }
	   }
	   if(flag==0){
	       cout<<"no"<<endl;
	   }
	   else{
	       cout<<"yes"<<endl;
	   }
	   
	}
	return 0;
}
