#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a,b;
	while(t--){
	    cin>>a>>b;
	    int d=a+b;
	    if(d<3){
	        cout<<"1"<<endl;
	    }
	    else if(d>=3 && d<=10){
	        cout<<"2"<<endl;
	    }
	    else if(d>=11 && d<=60){
	        cout<<"3"<<endl;
	    }
	    else{
	        cout<<"4"<<endl;
	    }
	}
	return 0;
}
