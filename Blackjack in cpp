#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a,b;
	while(t--){
	    cin>>a>>b;
	    int d=0;
	    int e=0;
	    d=a+b;
	    if(d>=11){
	        for(int i=0;i<=10;i++){
	            e=d+i;
	            if(e==21){
	                cout<<i<<endl;
	                break;
	            }
	        }
	    }
	    else{
	        cout<<"-1"<<endl;
	    }
	    
	}
	return 0;
}
