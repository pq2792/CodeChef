#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	
	while(t--){
	    int n;
	    
	    cin>>n;
	    char s[n];
	    for(int i=0;i<n;i++){
	        cin>>s[i];
	        if(s[i]=='A'){
	            s[i]='T';
	        }
	        else if(s[i]=='T'){
	            s[i]='A';
	        }
	       else  if(s[i]=='C'){
	            s[i]='G';
	        }
	        else if(s[i]=='G'){
	            s[i]='C';
	        }
	    }
	    for(int i=0;i<n;i++){
	        cout<<s[i];
	        
	    }
	    cout<<endl;
	    
	    
	}
	return 0;
}
