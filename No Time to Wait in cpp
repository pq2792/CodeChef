#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n,x,h,t[n];
	cin>>n>>x>>h;
	int flag=0;
	for(int i=0;i<n;i++){
	    cin>>t[i];
	    if(t[i]+h>=x){
	        flag=1;
	        break;
	    }
	}
	if(flag){
	    cout<<"YES"<<endl;
	}
	else{
	    cout<<"NO"<<endl;
	}
	
	return 0;
}
