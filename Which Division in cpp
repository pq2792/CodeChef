#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int r;
	while(t--){
	    cin>>r;
	    if(r<1600){
	        cout<<"3"<<endl;
	    }
	    else if(r>=1600 && r<2000){
	        cout<<"2"<<endl;
	    }
	    else{
	        cout<<"1"<<endl;
	    }
	}
	return 0;
}
