#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,x;
	    cin>>n>>x;
	    int a[n];
	    int sum=0;
	    for(int i=1;i<n;i++){
	        cin>>a[i];
	        sum+=a[i];
	    }
	    for(int i=0;i>=0;i++){
	        if(((sum+i)/n)>=x){
	            cout<<i<<endl;
	            break;
	        }
	    }
	}
	return 0;
}
