#include <iostream>
using namespace std;
int find(int n){
    int c=0;
    while(n>0){
        n=n/10;
        c++;
    }
    return c;
}

int main() {
	// your code goes here
	int n1;
	cin>>n1;
	int d=find(n1);
	if(d==1){
	    cout<<"1";
	}
	else if(d==2){
	    cout<<"2";
	}
	else if(d==3){
	    cout<<"3";
	}
	else{
	    cout<< "More than 3 digits" ;
	}
	return 0;
}

	
	
