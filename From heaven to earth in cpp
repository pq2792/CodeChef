#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int n,v1,v2;
	while(t--){
	    cin>>n>>v1>>v2;
	    double d=(n*2.0)/v2;
	    double e=(sqrt(2)*n/v1);
	   
	    if(d>e){
	        cout<<"Stairs"<<endl;
	    }
	    else{
	        cout<<"Elevator"<<endl;
	    }
	}
	return 0;
}
