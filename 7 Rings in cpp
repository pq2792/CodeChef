#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int n,x;
	while(t--)
	{
	    cin>>n>>x;
	    int d=n*x;
	    int c=0;
	    while(d>0)
	    {
	        int r=d%10;
	        c++;
	        d=d/10;
	    }
	    if(c==5)
	    {
	        cout<<"Yes"<<endl;
	    }
	    else
	    {
	        cout<<"No"<<endl;
	    }
	}
	return 0;
}
