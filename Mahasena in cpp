#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int n;
	cin>>n;
	int a[n];
	int c=0;
	int d=0;
	for(int i=0;i<n;i++){
	    cin>>a[i];
	}
	for(int i=0;i<n;i++){
	    if(a[i]%2==0){
	        c++;
	    }
	    else{
	        d++;
	    }
	}
	if(c>d){
	    cout<<"READY FOR BATTLE";
	}
	else{
	    cout<<"NOT READY";
	}
	return 0;
}
