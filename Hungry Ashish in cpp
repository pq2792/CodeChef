#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,z;
	    cin>>x>>y>>z;
	    if((x-y)>=0 && (x-z)>=0){
	        cout<<"pizza"<<endl;
	    }
	    else if((x-y)>=0 && (x-z)<0){
	        cout<<"pizza"<<endl;
	    }
	    else if((x-y)<0 && (x-z)>=0){
	        cout<<"burger"<<endl;
	    }
	    else{
	        cout<<"nothing"<<endl;
	    }
	}
	return 0;
}
