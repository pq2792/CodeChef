#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int a,b,c,d;
	while(t--){
	    cin>>a>>b>>c>>d;
	    int e=0;
	    int f=0;
	    e=a-c;
	    f=b-d;
	    if(e>f){
	        cout<<"second"<<endl;
	    }
	    else if(e<f){
	        cout<<"first"<<endl;
	    }
	    else{
	        cout<<"any"<<endl;
	    }
	}
	return 0;
}
