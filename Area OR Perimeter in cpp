#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int l,b;
	cin>>l>>b;
	int a=l*b;
	int p=(2*(l+b));
	if(p>a){
	    cout<<"Peri"<<endl;
	    cout<<p<<endl;
	}
	else if(p==a){
	    cout<<"Eq"<<endl;
	    cout<<p<<endl;
	}
	else{
	    cout<<"Area"<<endl;
	    cout<<a<<endl;
	}
	return 0;
}
