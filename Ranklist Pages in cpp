#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	int x;
	while(t--){
	    cin>>x;
	    if(x<=25){
	        cout<<"1"<<endl;
	    }
	    else if(x%25==0){
	        cout<<(x/25)<<endl;
	    }
	    else{
	        cout<<(x/25)+1<<endl;	    
	        
	    }
	}
	return 0;
}
